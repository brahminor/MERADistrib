<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
		
		<record model="ir.cron" id="seuil_alerte">
		    <field name='name'>Seuil alerte</field>
		    <field name='interval_number'>1</field>
		    <field name='interval_type'>minutes</field>
		    <field name="numbercall">-1</field>
		    <field name="active">True</field>
		    <field name="doall" eval="False"/>
		    <field name="model_id" ref="product.model_product_template"/>
		    <field name="state">code</field>
		    <field name="code">model.product_seuil_notif()</field>
		    <field name="user_id" ref="base.user_root"/> 
		</record>
		 <record model="ir.cron" id="enleve_seuil_alerte">
		    <field name='name'>Enlever Seuil alerte</field>
		    <field name='interval_number'>1</field>
		    <field name='interval_type'>minutes</field>
		    <field name="numbercall">-1</field>
		    <field name="active">True</field>
		    <field name="doall" eval="False"/>
		    <field name="model_id" ref="product.model_product_template"/>
		    <field name="state">code</field>
		    <field name="code">model.enleve_seuil_notif()</field>
		</record> 
	</data>
</odoo>